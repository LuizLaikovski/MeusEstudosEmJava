package _get_set_constr;
public class Caneta {
	public String modelo;
	public float ponta;
	
	public String getModelo() {
		return this.modelo;
	}
	
	public void setModelo(String m) {
		this.modelo = m;
	}
	
	public void setPonta(double p) {
		this.ponta = p;
	}
	
	public double getPonta() {
		return this.ponta;
	}
}
