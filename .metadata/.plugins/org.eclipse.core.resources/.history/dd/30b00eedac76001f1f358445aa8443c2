package Banco;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		double numc;
		String tipoC;
		String nome;
		boolean statusc;
		
		
		System.out.println("Digite o nome do proprietário da conta: "); numc = sc.nextInt();
		System.out.println("Digite o tipo de conta que você deseja criar: "); tipoC = sc.findInLine(tipoC);
		System.out.println("Digite o nome(completo) do proprietáio: "); nome = sc.findInLine(nome);
		System.out.println("Digite se a conta estará ativa ou não: "); statusc = sc.equals(statusc);
		
		ContaBanco cliente = new ContaBanco(numc, tipoC, nome, 0.0, statusc);
		
		cliente.status();
	}

}
