package Tarefas;

public class Main {

	public static void main(String[] args) {
		// CRIA UM NOVO OBJETO A PARTIR DA CLASSE CACHORRO
		Cachorro c1 = new Cachorro();
		
		// DA SUAS RESPECTIVAS CATEGORIAS
		c1.nome = "Demolidor";
		c1.raca = "Pincher";
		c1.idade = 4;
		
		// EXDCUTA OS MÃ‰TODOS
		c1.getnome();
		c1.getRaca();
		c1.IdadeEmAnosHumanos();
		System.out.printf("O cahorro tem "+ c1.idade +" e tem "+ c1.IdadeEmAnosHumanos() +" anos humanos de vida.");
		System.out.println();
		c1.Latir();

	}

}