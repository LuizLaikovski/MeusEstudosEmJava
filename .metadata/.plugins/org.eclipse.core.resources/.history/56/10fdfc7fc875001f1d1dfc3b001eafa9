package ContaBamcaria;

public class Main {

	public static void main(String[] args) {
		
		ContaBancaria minhaConta = new ContaBancaria(16012008, "Lucas Souza", 100000.0);
		
        System.out.println("Saldo inicial: R$ " + minhaConta.consultarSaldo());
        minhaConta.depositar(50000.0);
        System.out.println("Saldo após depósito: R$ " + minhaConta.consultarSaldo());
        minhaConta.sacar(120000.0);
        System.out.println("Saldo após saque: R$ " + minhaConta.consultarSaldo());

	}

}
